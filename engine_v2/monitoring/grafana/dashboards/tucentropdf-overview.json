{
  "dashboard": {
    "title": "TuCentroPDF Engine V2 - Overview",
    "tags": ["tucentropdf", "overview", "fase4"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Jobs Enqueued Rate (per second)",
        "targets": [
          {
            "expr": "rate(tucentropdf_jobs_enqueued_total[5m])",
            "legendFormat": "{{type}} - {{plan}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Jobs/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "pointradius": 2,
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Queue Length",
        "targets": [
          {
            "expr": "tucentropdf_queue_length",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Pending Jobs"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "thresholds": [
          {"value": 100, "colorMode": "critical", "op": "gt", "line": true, "fill": false}
        ],
        "alert": {
          "name": "Queue Length High",
          "conditions": [
            {"evaluator": {"params": [100], "type": "gt"}, "query": {"params": ["A", "5m", "now"]}}
          ]
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Job Success vs Failure Rate",
        "targets": [
          {
            "expr": "rate(tucentropdf_jobs_completed_total[5m])",
            "legendFormat": "Completed - {{type}}",
            "refId": "A"
          },
          {
            "expr": "rate(tucentropdf_jobs_failed_total[5m])",
            "legendFormat": "Failed - {{type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Jobs/sec"},
          {"format": "short"}
        ],
        "seriesOverrides": [
          {"alias": "/Failed/", "color": "#F2495C"}
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Job Processing Duration (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tucentropdf_job_duration_seconds_bucket[5m]))",
            "legendFormat": "{{type}} - p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(tucentropdf_job_duration_seconds_bucket[5m]))",
            "legendFormat": "{{type}} - p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "type": "stat",
        "title": "Worker Health",
        "targets": [
          {
            "expr": "tucentropdf_worker_health",
            "legendFormat": "{{worker}} - {{instance}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "auto",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "HEALTHY"}
            ]
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "type": "graph",
        "title": "Worker Processing Time",
        "targets": [
          {
            "expr": "rate(tucentropdf_worker_processing_seconds_sum[5m]) / rate(tucentropdf_worker_processing_seconds_count[5m])",
            "legendFormat": "{{worker}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Avg Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "type": "graph",
        "title": "Worker Error Rate",
        "targets": [
          {
            "expr": "rate(tucentropdf_worker_errors_total[5m])",
            "legendFormat": "{{worker}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "seriesOverrides": [
          {"alias": "/.*/", "color": "#F2495C"}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "type": "graph",
        "title": "HTTP Request Rate",
        "targets": [
          {
            "expr": "rate(tucentropdf_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status_code}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "type": "graph",
        "title": "HTTP Latency (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tucentropdf_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}} - p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 5, "colorMode": "critical", "op": "gt", "line": true}
        ]
      },
      {
        "id": 10,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 32},
        "type": "stat",
        "title": "Total Jobs Enqueued (24h)",
        "targets": [
          {
            "expr": "sum(increase(tucentropdf_jobs_enqueued_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 32},
        "type": "stat",
        "title": "Success Rate (24h)",
        "targets": [
          {
            "expr": "sum(increase(tucentropdf_jobs_completed_total[24h])) / sum(increase(tucentropdf_jobs_enqueued_total[24h])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 32},
        "type": "stat",
        "title": "Avg Processing Time (5m)",
        "targets": [
          {
            "expr": "avg(rate(tucentropdf_job_duration_seconds_sum[5m]) / rate(tucentropdf_job_duration_seconds_count[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 32},
        "type": "stat",
        "title": "Active Workers",
        "targets": [
          {
            "expr": "count(tucentropdf_worker_health == 1)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "yellow"},
                {"value": 2, "color": "green"}
              ]
            }
          }
        }
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "templating": {"list": []},
    "annotations": {"list": []},
    "links": []
  }
}
